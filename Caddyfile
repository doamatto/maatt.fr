(redirs) {
	# Old page URLs
	redir /archive/social /contact/
	redir /archive/support /support/

	# Old project URLS
	redir /always-amazon-smiling /projects/always-amazon-smiling
	redir /bashin /archive/bashin/
	redir /bittalk /archive/bashin/

	# Caching
	header Cache-Control max-age=3600
	header /static/* Cache-Control max-age=31536000

	# General headers
	Permissions-Policy interest-cohort=()
	X-Content-Type-Options nosniff
	X-Frame-Options DENY
	Referrer-Policy no-refrrer
	Strict-Transport-Security max-age=31536000;includeSubDomains
	#Strict-Transport-Security max-age=31536000;includeSubDomains;preload
	header Vary Origin
	header X-XSS-Protection 1; mode=block
	header Expect-CT max-age=86400, enforce
	header Content-Security-Policy default-src 'none'; img-src data: *; font-src 'none'; form-action 'none'; upgrade-insecure-requests; block-all-mixed-content; object-src 'none'; connect-src 'none'; base-uri 'self'; frame-ancestors 'none'
}