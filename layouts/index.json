{
  "version": "https://jsonfeed.org/version/1",
  "title": "Matthew Ronchetto",
  "home_page_url": "https://maatt.fr",
  "feed_url": "https://maatt.fr/index.json",
  "description": "I'm a high school student living in San Diego. I like to mess around with software development and graphic design.",
  "author": { "name": "Matthew Ronchetto" },
  "items": [
    {{ range $i, $e := first 10 (where site.RegularPages "Type" "in" site.Params.mainSections) }}
    {{ if $i }}, {{ end }}
      {
        "id": "{{ .Permalink }}",
        "title": "{{ .Title }}",
        "content_text": {{ .Content | jsonify }},
        "url": "{{ .Permalink }}",
        "date_published": "{{ .Date }}"
      }
    {{ end }}
  ]
}
