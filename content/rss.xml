+++
title= "Atom"
type= "atom"
+++

<?xml version="1.0\" encoding="utf-8" standalone="yes"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Matthew Ronchetto</title>
    <link>https://maatt.fr/blog</link>
    <description>Recent content on maatt.fr</description>
    <generator>Hugo -- gohugo.io</generator>
    <language>en</language>
    <managingEditor>heyy@maatt.fr</managingEditor>
    <webMaster>heyy@maatt.fr</webMaster>
    <copyright>&copy; 2015-2022 Matthew Ronchetto. CC-BY, unless mentioned otherwise.</copyright>
    {{ if not .Date.IsZero }}
    <lastBuildDate>{{ .Date.Format "Mon, 02 Jan 2006 15:04:05 -0700" | safeHTML }}</lastBuildDate>
    {{ end }}
    <atom:link href="https://maatt.fr/atom.xml" rel="self" type="application/rss+xml" />
    {{ range first 30 (where site.RegularPages "Type" "in" site.Params.mainSections) }}
    <item>
      <title>{{ .Title }}</title>
      <link>{{ .Permalink }}</link>
      <pubDate>{{ .Date.Format "Mon, 02 Jan 2006 15:04:05 -0700" | safeHTML }}</pubDate>
      <author>Matthew Ronchetto</author>
      <guid>{{ .Permalink }}</guid>
      <description>{{ .Content | html }}</description>
    </item>
    {{ end }}
  </channel>
</rss>
